  end
  end
         end
     end
  end)
  UIS.InputBegan:Connect(function(i)
     if i.KeyCode == killkeybind then
         local Obj = Mouse.Target;
         if Obj ~= "Terrain" then
             for i,v in pairs(sharks:GetChildren()) do
                 local sharksPos = v.PrimaryPart.Position
